(function() {
  var change_bg;

  change_bg = function(i) {
    return setTimeout((function() {
      var imgs;
      imgs = ["<%= asset_path('home/cri.jpg') %>", "<%= asset_path('home/equipe_royan_2014.jpg') %>", "<%= asset_path('home/martigua_sam.jpg') %>", "<%= asset_path('home/martigua_tip.jpg') %>", "<%= asset_path('home/martigua2_echauffement.jpg') %>", "<%= asset_path('home/martigua2.jpg') %>"];
      $(".img_bg").fadeTo(1000, 0, function() {
        return $(this).css("background-image", "url(" + imgs[i % imgs.length] + ")");
      }).fadeTo("slow", 1);
      return change_bg(i + 1);
    }), 8000);
  };

  $(document).ready(function() {
    if ($(".img_bg").length > 0) {
      return change_bg(0);
    }
  });

}).call(this);
